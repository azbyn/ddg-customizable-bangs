{
  "manifest_version": 2,
  "name": "DuckDuckGo customizable !bangs",
  "version": "0.2.1",
  "description": "Processes DuckDuckGo !bangs client-side where can modify them",
  "icons": {
    "16": "icons/icon-16.png",
    "24": "icons/icon-24.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://duckduckgo.com/*"
    // "https://duckduckgo.com/bang.js"
  ],
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "gen.js"
    ]
  }
}
